<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView_s"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="143dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="140dp"
        app:srcCompat="@drawable/usuario"
        tools:srcCompat="@drawable/ic_account_circle_black_24dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="106dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView_s"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="267dp"
        android:text="@string/etNombre"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvPatientNombre"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="290dp"
        android:text="@string/etApellido"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="168dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvPatientApellido"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="205dp"
        android:text="@string/etCorreo"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tvPatientGlucosa"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvPatientCorreo"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="275dp"
        android:text="@string/etContraseña"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:layout_below="@+id/tvPatientGlucosa"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="403dp"
        android:layout_marginBottom="365dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvPatientContraseña"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="202dp"
        android:text="Fecha de Nacimiento"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tvPatienteFecha"
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView11"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="155dp"
        android:text="TextView"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tvPatientCorreo"
        android:layout_width="294dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="71dp"
        android:text="TextView"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tvPatientContraseña"
        android:layout_width="307dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvPatientGlucosa"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="66dp"
        android:text="TextView"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tvPatientNombre"
        android:layout_width="317dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="56dp"
        android:text="TextView"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tvPatientApellido"
        android:layout_width="300dp"
        android:layout_height="23dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="72dp"
        android:text="TextView"
        android:textSize="18sp" />

</RelativeLayout>